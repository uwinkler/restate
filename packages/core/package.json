{
  "name": "@restate/core",
  "version": "0.13.3",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build",
    "start": "tsc --watch",
    "build": "rm -rf build && esbuild --outdir=build/cjs --format=cjs src/*.ts && esbuild --outdir=build/cjs --format=cjs src/*.tsx && esbuild --out-extension:.js=.js --outdir=build --format=esm src/*.ts && esbuild --out-extension:.js=.js --outdir=build --platform=node --format=esm src/*.tsx && tsc --emitDeclarationOnly",
    "test": "jest"
  },
  "main": "./build/cjs/index.js",
  "module": "./build/index.js",
  "typings": "./build/index.d.ts",
  "peerDependencies": {
    "immer": ">=9.0.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "rxjs": "^7.5.6"
  },
  "repository": "https://github.com/uwinkler/restate",
  "homepage": "https://restate.netlify.com/",
  "keywords": [
    "react",
    "restate",
    "state",
    "manager",
    "management",
    "redux",
    "store"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/history": "5.0.0",
    "@types/jest": "28.1.6",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-test-renderer": "18.0.0",
    "babel-jest": "28.1.3",
    "esbuild": "^0.15.3",
    "immer": "10.0.2",
    "jest": "28.1.3",
    "prettier": "^2.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "rxjs": "^7.5.6",
    "ts-jest": "^28.0.7",
    "typescript": "^5.2.2",
    "utility-types": "^3.7.0"
  },
  "gitHead": "8861457622e7037124bcbb0bd7f7c2a4585f88ab"
}
