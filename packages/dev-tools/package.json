{
  "name": "@restate/dev-tools",
  "version": "0.13.2",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build",
    "start": "tsc --watch",
    "build": "esbuild --outdir=build/cjs --platform=node --format=cjs src/*.ts && esbuild --out-extension:.js=.mjs --outdir=build --platform=node --format=esm src/*.ts && tsc --emitDeclarationOnly",
    "test": "jest"
  },
  "main": "build/cjs/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "repository": "https://github.com/uwinkler/restate",
  "homepage": "https://restate.netlify.com/dev-tools",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "restate",
    "state",
    "manager",
    "management",
    "redux",
    "store",
    "dev",
    "dev-tools"
  ],
  "peerDependencies": {
    "@restate/core": ">=0.13.0",
    "immer": ">=9.0.0",
    "rxjs": ">=7.5.6"
  },
  "devDependencies": {
    "@restate/core": "^0.13.2",
    "@types/jest": "^28.1.6",
    "esbuild": "^0.15.3",
    "immer": "^9.0.15",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "rxjs": "^7.5.6",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "gitHead": "77a4516a4f23a89f15f8de5bd6f0b5fcc50f468c"
}
