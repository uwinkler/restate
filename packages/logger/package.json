    "@types/jest": "28.1.6",
    "babel-jest": "^28.1.3",
    "esbuild": "^0.15.3",
    "immer": "^9.0.15",
    "jest": "^28.1.3",
    "prettier": "^2.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.5.6",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4",
    "utility-types": "^3.10.0"
  },
  "gitHead": "77a4516a4f23a89f15f8de5bd6f0b5fcc50f468c",
